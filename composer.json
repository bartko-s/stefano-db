{
    "name": "stefano/stefano-db",
    "description": "Db Modul extend Zend Framework 2",
    "version": "1.5.0",
    "license": "BSD-3-Clause",
    "keywords": [
        "Nested Db Transaction"
    ],
    "support": {
        "source": "https://github.com/bartko-s/stefano-db",
        "issues": "https://github.com/bartko-s/stefano-db/issues"
    },
    "homepage": "https://github.com/bartko-s/stefano-db",
    "authors": [
        {
            "name": "Å tefan Bartko"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "zendframework/zend-servicemanager": "^3.0.0",
        "zendframework/zend-db": "^2.0.0",
        "stefano/stefano-nested-transaction": "^0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7 <10",
        "mockery/mockery": "^1.0.0",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-0": {
            "StefanoDb\\": "src/"
        },
        "classmap": [
            "./src/"
        ]
    },
    "autoload-dev": {
      "psr-0": {
        "StefanoDbTest\\": "tests/"
      }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --configuration tests/phpunit.xml"
    }
}
